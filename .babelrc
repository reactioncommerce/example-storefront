{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current",
          "browsers": [
            "last 2 versions"
          ]
        }
      }
    ],
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    ["module-resolver", {
      "root": [ "./src" ]
    }]
  ]
}