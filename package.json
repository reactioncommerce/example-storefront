{
  "name": "reaction-next-starterkit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": {
    "name": "Reaction Commerce",
    "email": "engineering@reactioncommerce.com",
    "url": "https://reactioncommerce.com"
  },
  "license": "GPL-3.0",
  "scripts": {
    "dev": "NODE_ENV=development babel-node ./src/server.js",
    "build": "yarn run build-clean && yarn run build-transpile && yarn run build-sync && next build src && cp -R src/.next build/app",
    "build-transpile": "NODE_ENV=production babel src -d build/app --ignore '**/*.test.js','**/*__mocks__','**/*__snapshots__'",
    "build-sync": "cp -R src/static build/app/static && rsync -av --prune-empty-dirs --include='*/' --include='*.gql' --exclude='*' src/* build/app",
    "build-clean": "rimraf build/app",
    "build-next": "yarn install --save-dev && next build src",
    "lint": "eslint .",
    "start": "NODE_ENV=production node build/app/server.js",
    "test": "yarn run test:unit",
    "test:ci": "yarn run test:unit --ci --runInBand --colors",
    "test:unit": "NODE_ENV=jesttest jest",
    "test:unit:watch": "NODE_ENV=jesttest jest --watchAll",
    "test:integration": "mocha tests/integration",
    "test:link": "blc http://web.api.reaction.localhost:4000 -ro -filter=3 -e"
  },
  "eslintConfig": {
    "extends": [
      "@reactioncommerce"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^3.1.0",
    "@reactioncommerce/components": "0.45.2",
    "@reactioncommerce/components-context": "^1.0.0",
    "@segment/snippet": "^4.3.1",
    "apollo-cache-inmemory": "^1.1.11",
    "apollo-client": "^2.2.7",
    "apollo-link": "^1.2.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.0.9",
    "apollo-link-http": "^1.5.4",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.2",
    "classnames": "^2.2.5",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "keymirror": "^0.1.1",
    "mdi-material-ui": "^5.4.0",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "next": "^7.0.2",
    "next-routes": "^1.4.2",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.4.0",
    "passport-oauth2-refresh": "^1.1.0",
    "prop-types": "^15.6.2",
    "react": "16.4.2",
    "react-apollo": "^2.1.3",
    "react-container-query": "^0.11.0",
    "react-dom": "16.4.2",
    "react-helmet": "^5.2.0",
    "react-stripe-elements": "^2.0.1",
    "react-tracking": "^5.4.1",
    "reacto-form": "^0.0.2",
    "request": "^2.88.0",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@reactioncommerce/eslint-config": "^1.7.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.6.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.7.1",
    "broken-link-checker": "^0.7.8",
    "chai": "^4.1.2",
    "chai-cheerio": "^1.0.0",
    "cheerio": "^1.0.0-rc.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^23.6.0",
    "jest-junit": "^3.6.0",
    "jest-transform-graphql": "2.1.0",
    "mocha": "^5.2.0",
    "react-test-renderer": "^16.3.1",
    "rimraf": "^2.6.2",
    "snyk": "^1.73.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/reports/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "reports/coverage",
    "testResultsProcessor": "jest-junit",
    "roots": [
      "./src"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest"
    }
  },
  "jest-junit": {
    "output": "reports/junit/junit.xml",
    "suiteName": "jest-tests"
  }
}
